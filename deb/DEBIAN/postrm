#!/bin/bash
USBDEFAULT="/usr/local/lib/usb_default"
USBUNKNOWN="/usr/local/lib/usb_unknown"

#Remove usb_default configuration
if [ -f $USBDEFAULT ]
then
	rm $USBDEFAULT
fi

#Remove usb_unknown configuration
if [ -f $USBUNKNOWN ]
then
	rm $USBUNKNOWN
fi

#Recreate usr/local/sbin directory
if [ ! -d "/usr/local/sbin/" ]
then
	mkdir /usr/local/sbin/
fi
